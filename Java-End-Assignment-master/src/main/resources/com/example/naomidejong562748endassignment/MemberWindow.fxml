<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
       <GridPane prefHeight="400.0" prefWidth="506.0" style="-fx-background-color: #f3f5ff;">
           <columnConstraints>
               <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
           </columnConstraints>
           <rowConstraints>
               <RowConstraints maxHeight="195.60001220703126" minHeight="10.0" prefHeight="85.20001831054688" vgrow="SOMETIMES" />
               <RowConstraints maxHeight="316.4" minHeight="10.0" prefHeight="234.4000122070312" vgrow="SOMETIMES" />
               <RowConstraints maxHeight="316.4" minHeight="10.0" prefHeight="79.99998779296874" vgrow="SOMETIMES" />
           </rowConstraints>
           <children>
               <Label alignment="CENTER" text="Members" textFill="#272933" GridPane.halignment="LEFT" GridPane.valignment="BOTTOM">
                   <font>
                       <Font name="Times New Roman" size="24.0" />
                   </font>
                   <GridPane.margin>
                       <Insets bottom="20.0" left="20.0" top="10.0" />
                   </GridPane.margin>
               </Label>
               <TableView fx:id="memberTable" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                   <columns>
                       <TableColumn prefWidth="75.0" resizable="false" text="Identifier">
                           <cellValueFactory>
                               <PropertyValueFactory property="identifier" />
                           </cellValueFactory>
                       </TableColumn>
                       <TableColumn prefWidth="130.0" resizable="false" text="First Name">
                           <cellValueFactory>
                               <PropertyValueFactory property="firstName" />
                           </cellValueFactory>
                       </TableColumn>
                       <TableColumn prefWidth="130.0" resizable="false" text="Last Name">
                           <cellValueFactory>
                               <PropertyValueFactory property="lastName" />
                           </cellValueFactory>
                       </TableColumn>
                       <TableColumn prefWidth="130.0" resizable="false" text="Birthday">
                            <cellValueFactory>
                                <PropertyValueFactory property="birthday" />
                            </cellValueFactory>
                       </TableColumn>
                   </columns>
                   <GridPane.margin>
                       <Insets left="20.0" right="20.0" />
                   </GridPane.margin>
               </TableView>
               <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="2">
                   <children>
                       <Button fx:id="addMemberButton" mnemonicParsing="false" onAction="#addMemberButtonClick" prefHeight="30.0" prefWidth="200.0" style="-fx-background-color: #272933;" text="Add Member" textFill="#f3f5ff">
                           <font>
                               <Font name="Times New Roman" size="12.0" />
                           </font>
                           <HBox.margin>
                               <Insets left="20.0" right="20.0" />
                           </HBox.margin>
                       </Button>
                       <Button fx:id="editMemberButton" mnemonicParsing="false" onAction="#editMemberButtonClick" prefHeight="30.0" prefWidth="200.0" style="-fx-background-color: #272933;" text="Edit Member" textFill="#f3f5ff">
                           <font>
                               <Font name="Times New Roman" size="12.0" />
                           </font>
                           <HBox.margin>
                               <Insets left="20.0" right="20.0" />
                           </HBox.margin>
                       </Button>
                       <Button fx:id="deleteMemberButton" mnemonicParsing="false" onAction="#deleteMemberButtonClick" prefHeight="30.0" prefWidth="200.0" style="-fx-background-color: #272933;" text="Delete Member" textFill="#f3f5ff">
                           <font>
                               <Font name="Times New Roman" size="12.0" />
                           </font>
                           <HBox.margin>
                               <Insets left="20.0" right="20.0" />
                           </HBox.margin>
                       </Button>
                   </children>
               </HBox>
           </children>
       </GridPane>
   </children>
</HBox>
